#include<stdio.h>
int main()
{
    float s1,s2,s3;
    float avg,total;
    char grade;
    s1=10;
    s2=20;
    s3=30;
    scanf("%f %f %f", &s1, &s2, &s3);
    total= s1+s2+s3;
    avg=(s1+s2+s3)/3;
    if(avg>=90)
    grade = 'A';
    else if(avg>=75)
    grade = 'B';
    else if(avg>=60)
    grade = 'C';
    else if(avg>=40)
    grade = 'D';
    else
    grade = 'F';
    printf("\nThe total marks of three subject is: %f", total);
    printf("\nThe average of three subjects marks is: %f", avg);
    printf("\nthe Grade is: %c", grade);
    return 0;
    
}
